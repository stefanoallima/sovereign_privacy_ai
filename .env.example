# Private Personal Assistant - Environment Variables
# Copy this file to .env and fill in your values

# Required: Nebius API Key for LLM
NEBIUS_API_KEY=your_nebius_api_key_here

# Optional: LiveKit Configuration (defaults shown)
LIVEKIT_URL=ws://localhost:7880
LIVEKIT_API_KEY=devkey
LIVEKIT_API_SECRET=secret

# Optional: Model paths (auto-configured by start-local.ps1)
# PIPER_MODEL_PATH=C:\path\to\models\en_US-lessac-medium.onnx

# Optional: LLM Model (default: Qwen3-235B)
# LLM_MODEL=Qwen/Qwen3-235B-A22B
